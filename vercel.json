{
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/" }
  ],
  "rewrites": [
    {
      "source": "/:path((?!api/).*)",
      "destination": "/"
    },
    {
      "source": "/api/:path*",
      "destination": "https://diotviet-xkqlatwh.b4a.run/api/:path*"
    }
  ],
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vue"
}